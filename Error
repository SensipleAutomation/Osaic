    WebElement element = RecleSource;
    JavascriptExecutor js = (JavascriptExecutor) driver;
    js.executeScript("arguments[0].scrollIntoView();", element);
    if (isElementPresent(RecleSource)) {
        RecleSource.click();
        LeadMarket.click();
        TestUtils.sleep(1000);
    }
}

