for (int i = 0; i < 5; i++) {
            // Scroll down using JavaScript
            scrollDown(driver);

            // Wait for a short time to allow the page to load
            try {
                Thread.sleep(1000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }

            // Find the element and click on it
            WebElement element = driver.findElement(elementLocator);
            element.click();
        }
