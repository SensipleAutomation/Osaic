int totalQuestions = Math.min(backConsentyesBtn.size(), backConsentTextbox.size());
		for (int i = 6; i < totalQuestions; i++) {

			WebElement yesButton = backConsentyesBtn.get(i);
			((JavascriptExecutor) driver).executeScript("arguments[0].scrollIntoView({block: 'center'});", yesButton);
			yesButton.click();
			WebDriverWait wait = new WebDriverWait(driver, 10);
			wait.until(ExpectedConditions.visibilityOf(backConsentTextbox.get(i)));

			backConsentyesBtn.get(i).sendKeys(Value + (i + 1));
		}
