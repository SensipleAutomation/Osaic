@FindBy(xpath="//web-selection-button//span[text()='No']")
	public List<WebElement> noBtn;

	@FindBy(xpath="//web-selection-button//span[text()='Yes']")
	public List<WebElement> yesBtn;

public WebElement clickBCQuestion1No() {
		return noBtn.get(0);
	}

