Assert.assertTrue(isElementPresent(boBusCase), "Bo Bus Case button is not present");
boBusCase.click();
reactions();
Thread.sleep(2000);

// Refresh
refreshCase();
Thread.sleep(1000);

// Pull back
pullback();
Thread.sleep(2000);

// Refresh again
refresh();
Thread.sleep(1000);

// Aff Matrix
Assert.assertTrue(isElementPresent(reviewBackgroundCheckandMatrixcase), "Aff Matrix button is not present");
reviewBackgroundCheckandMatrixcase.click();
reactions();
Thread.sleep(1000);

// OBA Refresh
casesOBA.click();
