if (extID.isDisplayed()) {
			String text = extID.getText();
			System.out.println("External Id:" + text);

		} else {
			driver.navigate().refresh();
			if (extID.isDisplayed()) {
				String text = extID.getText();
				System.out.println("External Id:" + text);
			throw new RuntimeException("External ID is not generated. Stopping automation testing.");
		}
