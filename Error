for (int i = 0; i < 6; i++) {
            try {
                elementToClick.click();
                System.out.println("Clicked " + (i+1) + " times");
                // Add a delay if needed
                Thread.sleep(1000); // Wait for 1 second (1000 milliseconds) between clicks
            } catch (Exception e) {
                e.printStackTrace();
            }
        }
