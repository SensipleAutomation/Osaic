WebElement refreshButtonLocator = refresh;
		WebElement anotherButtonLocator = refresClick;

		WebElement targetElementLocator = case2;

		while (true) {
			try {

				refreshButtonLocator.click();
				anotherButtonLocator.click();
				wait.withTimeout(java.time.Duration.ofSeconds(2))
						.until(ExpectedConditions.elementToBeClickable(targetElementLocator));
				targetElementLocator.click();

				break;
			} catch (Exception e) {

				e.printStackTrace();

			}
		}
