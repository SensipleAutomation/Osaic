try {
            Robot robot = new Robot();

            // Press and release 'O'
            robot.keyPress(KeyEvent.VK_O);
            robot.keyRelease(KeyEvent.VK_O);

            // Press and release 'K'
            robot.keyPress(KeyEvent.VK_K);
            robot.keyRelease(KeyEvent.VK_K);

        } catch (AWTException e) {
            e.printStackTrace();
        }
