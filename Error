try {

			Thread.sleep(2000);

			// Find the table and extract data
			WebElement table = driver.findElement(By.id("countries"));
			List<List<String>> tableData = new ArrayList<>();
			List<WebElement> rows = table.findElements(By.tagName("tr"));
			for (WebElement rowElement : rows) {
				List<String> rowData = new ArrayList<>();
				List<WebElement> cells = rowElement.findElements(By.tagName("td"));
				for (WebElement cell : cells) {
					rowData.add(cell.getText());
				}
				tableData.add(rowData);
			}

			// Write data to Excel
			String filePath = "C:\\Users\\rajasekar.p\\eclipse-workspace\\DataDriven\\Test-Data\\Test_Data.xlsx";
			writeDataToExcel(tableData, filePath);
		} catch (Exception e) {
			e.printStackTrace();
		} finally {
			driver.quit();
		}
	}

	public static void writeDataToExcel(List<List<String>> data, String filePath) {
		try (XSSFWorkbook workbook = new XSSFWorkbook()) {
			Sheet sheet = workbook.createSheet("Data");
			int rowCount = 0;
			for (List<String> rowData : data) {
				Row row = sheet.createRow(rowCount++);
				int cellCount = 0;
				for (String item : rowData) {
					Cell cell = row.createCell(cellCount++);
					cell.setCellValue(item);
				}
			}
			try (FileOutputStream fileOut = new FileOutputStream(filePath)) {
				workbook.write(fileOut);
			}
			System.out.println("Data has been written to Excel successfully.");
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
