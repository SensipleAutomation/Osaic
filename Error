public void newButton() {
    TestUtils.sleep(3000);
    if (isElementPresent(NewButton)) {
        Assert.assertTrue(NewButton.isDisplayed(), "New button is not present");
        NewButton.click();
    }
}

public void NewHead() {
    TestUtils.sleep(2000);
    if (isElementPresent(NewLeadHeader)) {
        Assert.assertTrue(NewLeadHeader.isDisplayed(), "New Header is not present");
        TestUtils.sleep(1000);
    }
}

public void BusinessProfile() {
    WebElement element = ScrollBusiness;
    JavascriptExecutor js = (JavascriptExecutor) driver;
    js.executeScript("arguments[0].scrollIntoView();", element);
    if (isElementPresent(ScrollBusiness)) {
        Assert.assertTrue(ScrollBusiness.isDisplayed(), "Business Profile Header is not present");
    }
}

public void Assigne() {
    WebElement element = scrollAssign;
    JavascriptExecutor js = (JavascriptExecutor) driver;
    js.executeScript("arguments[0].scrollIntoView();", element);
    if (isElementPresent(scrollAssign)) {
        Assert.assertTrue(scrollAssign.isDisplayed(), "Assign is not present");
    }
}

public void External() {
    WebElement element = scrollAssign;
    JavascriptExecutor js = (JavascriptExecutor) driver;
    js.executeScript("arguments[0].scrollIntoView();", element);
    if (isElementPresent(ExtRecNam)) {
        ExtRecNam.click();
        ExtRecNamSel.click();
        TestUtils.sleep(1000);
    }
}

public void Internal() {
    if (isElementPresent(IntRecNam)) {
        IntRecNam.click();
        IntRecNamSel.click();
        TestUtils.sleep(1000);
    }
}

public void wealth() {
    if (isElementPresent(AWMFirmDrop)) {
        AWMFirmDrop.click();
        dropOsaic.click();
        TestUtils.sleep(1000);
    }
}

