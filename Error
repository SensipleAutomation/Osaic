for (int i = 0; i < 26; i++) {
            // Get the "No" button at the current index
            WebElement noButton = page.noBtn.get(i);

            // Scroll the "No" button into view
            ((JavascriptExecutor) driver).executeScript("arguments[0].scrollIntoView({block: 'center'});", noButton);

            // Click the "No" button
            noButton.click();
