	public void clickBCQuestionSNoButtons() {
		int count = 23;
		for(int i=0; i<count; i++){
		noBtn.get(i).click();
		}
		TestUtils.sleep(2000);
	}

	public void clickBCQuestionSNoButtons(String count) {
		int num = Integer.parseInt(count)-1;
		for(int i=0; i<num; i++){
		noBtn.get(i).click();
		}
		TestUtils.sleep(2000);
	}

	public void clickBCQuestionSNoButtonsStartingFromTillEnd(String count) {
		int num = Integer.parseInt(count)+1;
		int endNum = noBtn.size()-1;
		for(int i=num; i==endNum; i++){
		noBtn.get(i).click();
		}
		subjectToHeightenedSupervisionLabel.click();
		TestUtils.sleep(2000);
	}
